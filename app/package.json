{
  "name": "reclaimed-fe",
  "version": "0.22.206",
  "description": "Reclaimed App Front End",
  "keywords": [
    "candee",
    "generations",
    "camp",
    "software",
    "react"
  ],
  "author": {
    "name": "Tyler Candee",
    "email": "tyler@candeegenerations.com",
    "url": "https://candeegenerations.com"
  },
  "license": "MIT",
  "scripts": {
    "start": "npm rebuild node-sass && nodemon -w craco.config.js -w ./src/content/antd.less --exec 'craco start'",
    "build": "rimraf build && REACT_APP_VERSION=$npm_package_version craco build",
    "test": "craco test",
    "test:ci": "CI=true yarn test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!antd)/.+\\.js$"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm version patch && git add package.json && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@craco/craco": "^5.6.4",
    "@emotion/core": "^10.0.28",
    "antd": "^3.26.18",
    "axios": "^0.21.1",
    "craco-antd": "^1.18.1",
    "deep-equal": "^2.0.3",
    "deepmerge": "^4.2.2",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.6",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "10.0.2",
    "react-dom": "^16.13.1",
    "react-router5": "^8.0.1",
    "react-scripts": "3.4.1",
    "rimraf": "^3.0.2",
    "router5": "^8.0.1",
    "router5-helpers": "^8.0.0",
    "router5-plugin-browser": "^8.0.1",
    "router5-plugin-logger": "^8.0.1",
    "simple-crypto-js": "^3.0.0",
    "use-deep-compare-effect": "^1.3.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "babel-jest": "24.9.0",
    "babel-plugin-emotion": "^10.0.33",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "customize-cra": "^1.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.2.5",
    "less": "^3.12.2",
    "less-loader": "^6.2.0",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4"
  }
}
